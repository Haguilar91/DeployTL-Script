server{
    listen 80 default_server;
    listen [::]:80 default_server;
    server_name 0.0.0.0;

    # Tell Nginx and Passenger where your app's 'public' directory is
    root #Lugar donde el public directory de la app esta;

    # Turn on passenger
    passenger_enabled on;
    passenger_ruby #correr which ruby para obtener este path;

}
